{
  "name": "uw-madison-data-extractors",
  "version": "1.0.0",
  "description": "Comprehensive data extraction system for UW-Madison academic data from MadGrades, Course Search & Enroll, and Rate My Professor",
  "type": "module",
  "main": "src/extractors/course_search_and_enroll_extractor.js",
  "scripts": {
    "extract:madgrades": "node src/extractors/madgrades_extractor.js",
    "extract:courses": "node src/extractors/course_search_and_enroll_extractor.js",
    "extract:rmp": "node src/extractors/rmp-extractor.js",
    "preprocess:rmp": "python src/preprocessors/rmp_preprocessor.py",
    "db:import": "node src/utils/run_dumps.js",
    "test": "node tests/test.js",
    "extract:all": "npm run extract:madgrades && npm run extract:courses && npm run extract:rmp"
  },
  "keywords": [
    "uw-madison",
    "data-extraction",
    "madgrades",
    "rate-my-professor",
    "course-data",
    "academic-data"
  ],
  "author": "Badger Base",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^17.2.2",
    "mysql2": "^3.14.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Badger-Base/data-extractors.git"
  }
}
